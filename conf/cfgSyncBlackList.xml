		<section name="new_firmware">
			<option name="fw_new_notify"></option>
			<option name="fw_update_type"></option>
			<option name="not_show"></option>
			<option name="display_type"></option>
			<option name="retained_message_bar"></option>
			<option name="notify_time"></option>
