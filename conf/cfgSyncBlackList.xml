<root>
<moduler name="network">
	<table name="interface" element="private">
		<section name="lan">
			<option name="ipaddr"></option>
			<option name="netmask"></option>
			<option name="fac_ipaddr"></option>
			<option name="fac_netmask"></option>
			<option name="ip_mode"></option>
			<option name="macaddr"></option>
		</section>
	</table>
</moduler>

<moduler name="uhttpd">
	<table name="webPwd" element="private">
		<section name="webPwd">
			<option name="fac_password"></option>
		</section>
	</table>
	<table name="uhttpd" element="private">
		<section name="main">
			<option name="listen_http_lan"></option>
			<option name="listen_http_wan"></option>
		</section>
	</table>
</moduler>

<moduler name="protocol">
	<table name="proto" element="private">
