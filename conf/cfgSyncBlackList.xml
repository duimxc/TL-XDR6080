		</section>
		<section name="connect_option">
			<option name="heartBeatInterval"></option>
			<option name="connectInterval"></option>
		</section>
		<section name="login">
			<option name="username"></option>
			<option name="password"></option>
		</section>
		<section name="register">
			<option name="username"></option>
			<option name="password"></option>
			<option name="verify_code"></option>
			<option name="account_type"></option>
		</section>
		<section name="reset_cloud_pwd">
			<option name="username"></option>
			<option name="password"></option>
			<option name="verify_code"></option>
			<option name="account_type"></option>
