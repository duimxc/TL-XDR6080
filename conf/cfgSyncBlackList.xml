			<option name="illegal"></option>
			<option name="illegal_type"></option>
		</section>
	</table>
	<table name="push_list" element="public">
		<option name="mac"></option>
		<option name="count"></option>
	</table>
	<table name="cloud_reply" element="private">
		<section name="device_status">
			<option name="bind_status"></option>
